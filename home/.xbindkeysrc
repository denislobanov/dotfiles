# browser
"xvkbd -xsendevent -text "\A\[Left]""
    m:0x0 + c:67
    F1 

"xvkbd -xsendevent -text "\A\[Right]""
    m:0x0 + c:68
    F2 

"xvkbd -xsendevent -text "\Cr""
    m:0x0 + c:69
    F3 

# display
"xvkbd -xsendevent -text "\[F11]""
    m:0x0 + c:70
    F4

# skipping F5 as i have no idea what to do with it for now

"xbacklight -dec 5"
    m:0x0 + c:72
    F6 

"xbacklight -inc 5"
    m:0x0 + c:73
    F7

# sound
"amixer set Master toggle"
    m:0x0 + c:74
    F8

"amixer set Master 5- unmute"
    m:0x0 + c:75
    F9

"amixer set Master 5+ unmute"
    m:0x0 + c:76
    F10

# editing
"xvkbd -xsendevent -text '\[Delete]'"
    m:0x8 + c:64
    Alt + Alt_L

"xvkbd -xsendevent -text '\[Home]'"
    m:0x8 + c:113
    Alt + Left

"xvkbd -xsendevent -text '\[End]'"
    m:0x8 + c:114
    Alt + Right

"xvkbd -xsendevent -text '\[Page_Up]'"
    m:0x8 + c:111
    Alt + Up

"xvkbd -xsendevent -text '\[Page_Down]'"
    m:0x8 + c:116
    Alt + Down

