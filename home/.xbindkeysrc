    Alt + F1

"xvkbd -xsendevent -text "[F2]""
    m:0x8 + c:68
    Alt + F2

"xvkbd -xsendevent -text "[F3]""
    m:0x8 + c:69
    Alt + F3

"xvkbd -xsendevent -text "[F4]""
    m:0x8 + c:70
    Alt + F4

"xvkbd -xsendevent -text "[F5]""
